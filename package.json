{
  "name": "@blitz/eslint-plugin",
  "version": "0.0.40",
  "description": "An ESLint config to enforce a consistent code styles across StackBlitz projects",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "stackblitz"
  ],
  "author": "Dominic Elm",
  "main": "dist/index.js",
  "files": [
    "dist",
    "docs",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "jest",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
